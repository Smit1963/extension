{
  "name": "Vs-llm-assiatnt",
  "displayName": "VsPilot",
  "description": "A VSCode extension that integrates a coding assistant LLM",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:llm-assistant.ask"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "llm-assistant.ask",
        "title": "Ask LLM Assistant"
      }
    ],
    "keybindings": [
      {
        "command": "llm-assistant.ask",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
